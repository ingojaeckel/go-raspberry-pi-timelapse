[connection]
id=wifi
type=wifi
interface-name=wlan0
autoconnect=true

[wifi]
mode=ap
ssid=timelapse-raspberry-pi

[wifi-security]
key-mgmt=wpa-psk
psk=InsertTheRealPassword

[ipv4]
dns-search=
address1=192.168.50.1/24,192.168.50.1
method=shared

[ipv6]
dns-search=
addr-gen-mode=stable-privacy
method=ignore

[proxy]
